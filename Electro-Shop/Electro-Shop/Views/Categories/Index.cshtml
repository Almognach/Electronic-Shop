@model IEnumerable<Electro_Shop.Models.Category>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Categories</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                <a asp-action="List" asp-route-id="@item.Id">@Html.DisplayFor(modelItem => item.Name)</a>
                
            </td>
        </tr>
}
    </tbody>
</table>
